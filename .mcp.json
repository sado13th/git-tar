{
  "mcpServers": {
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "morph-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@morphllm/morphmcp"
      ],
      "env": {
        "MORPH_API_KEY": "",
        "ENABLED_TOOLS": "edit_file,warpgrep_codebase_search"
      }
    }
  }
}
